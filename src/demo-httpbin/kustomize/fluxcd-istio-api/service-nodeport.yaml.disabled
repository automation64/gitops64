---
apiVersion: "v1"
kind: "Service"
metadata:
  name: "demo-httpbin"
  namespace: "demo-httpbin"
  labels:
    app: "demo-httpbin"
    service: "demo-httpbin"
spec:
  # Enable minikube service export
  type: "NodePort"
  selector:
    app: "demo-httpbin"
  ports:
    - name: "http"
      port: 8080
      targetPort: 80
...
