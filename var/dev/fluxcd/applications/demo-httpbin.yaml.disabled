---
apiVersion: "kustomize.toolkit.fluxcd.io/v1"
kind: "Kustomization"
metadata:
  name: "demo-httpbin"
  namespace: "flux-system"
spec:
  interval: "1m0s"
  timeout: "5m0s"
  prune: true
  wait: true
  path: "./etc/dev/demo-httpbin/fluxcd-istio-api"
  dependsOn:
    - name: "istio-system"
  sourceRef:
    kind: "GitRepository"
    name: "flux-system"
...
