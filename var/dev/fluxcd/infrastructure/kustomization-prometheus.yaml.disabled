---
apiVersion: "kustomize.toolkit.fluxcd.io/v1"
kind: "Kustomization"
metadata:
  name: "prometheus"
  namespace: "flux-system"
spec:
  interval: "1m0s"
  timeout: "5m0s"
  prune: true
  wait: true
  path: "./etc/dev/prometheus/fluxcd"
  dependsOn:
    - name: "metrics-server"
    - name: "cert-manager"
    - name: "istio-system"
    - name: "rook"
  sourceRef:
    kind: "GitRepository"
    name: "flux-system"
...
